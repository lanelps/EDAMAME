{
  "name": "edamame",
  "version": "0.1.0",
  "main": "server/index.js",
  "repository": "https://github.com/harakeke-2019/edamame.git",
  "bugs": "https://github.com/harakeke-2019/edamame/issues",
  "author": "EDA",
  "license": "ISC",
  "scripts": {
    "start": "concurrently \"node server\" \"webpack --watch\"",
    "dev": "concurrently 'yarn client' 'yarn server'",
    "client": "webpack --watch",
    "server": "nodemon server --ignore server/public",
    "knex": "knex --knexfile ./knexfile.js",
    "updateDb": "rm -f dev.sqlite3 && yarn knex migrate:latest && yarn knex seed:run",
    "test": "jest --watchAll --testPathIgnorePatterns tests/cypress",
    "e2e": "nodemon server --ignore server/public & webpack --watch & cypress open"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tests/setup.js"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "bootstrap": "^4.3.1",
    "cheerio": "^1.0.0-rc.2",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.0",
    "knex": "^0.16.3",
    "libsodium-wrappers": "^0.7.9",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react-redux": "^6.0.1",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-circular-progressbar": "^1.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sqlite3": "^4.0.6",
    "superagent": "^4.1.0",
    "supertest": "^3.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "cypress": "^3.1.5",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.1"
  }
}
